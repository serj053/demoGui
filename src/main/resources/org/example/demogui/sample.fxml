<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.geometry.Point3D?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="CENTER" hgap="2.0" prefHeight="153.0" prefWidth="210.0" snapToPixel="false" vgap="2.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.demogui.Controller">
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="147.0" minWidth="10.0" prefWidth="147.0" />
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="76.625" minWidth="10.0" prefWidth="24.625" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
     <Label fx:id="label1" text="Text" GridPane.columnIndex="1" />
     <Button fx:id="btn1" onAction="#act1" text="action1" GridPane.columnIndex="2" />
      <Button fx:id="btn2" mnemonicParsing="false" text="action 2" GridPane.columnIndex="2" GridPane.rowIndex="1" />
      <Label text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Button fx:id="btn3" mnemonicParsing="false" text="action 3" GridPane.columnIndex="2" GridPane.rowIndex="2" />
      <Button fx:id="btn4" mnemonicParsing="false" text="action 4" GridPane.columnIndex="2" GridPane.rowIndex="3" />
      <TextField fx:id="textField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <Label fx:id="label2" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="3" />
      <HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="4">
         <children>
            <Button fx:id="boxPlus" mnemonicParsing="false" text="+" />
            <Button fx:id="boxMinus" mnemonicParsing="false" text="-" />
         </children>
      </HBox>
      <TextField fx:id="boxWeight" GridPane.columnIndex="2" GridPane.rowIndex="4" />
      <Button fx:id="btnToTable" mnemonicParsing="false" text="Перейти к таблицам" GridPane.columnIndex="1" GridPane.rowIndex="5" />
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <rotationAxis>
      <Point3D />
   </rotationAxis>
</GridPane>
